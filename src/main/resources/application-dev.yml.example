spring:
  datasource:
    driver-class-name: org.postgresql.Driver
    url: jdbc:postgresql://localhost:5456/zenfood
    username: postgres
    password: 123456
    platform: postgres

  jpa:
    show-sql: true
    properties:
      hibernate:
        dialect: org.hibernate.dialect.PostgreSQLDialect

app:
  jwtSecret: JWTSuperSecretKey
  jwtExpirationInMs: 604800000

logging:
  level:
    org:
      hibernate:
        SQL: DEBUG

server:
  port: 8090

